{
  "branches": ["main"],
  "tagFormat": "release/${version}",
  "plugins": [
    ["@semantic-release/commit-analyzer", { "preset": "angular" }],
    "@semantic-release/release-notes-generator",
    ["@semantic-release/changelog", { "preset": "angular" }],
    ["@semantic-release/npm", { "npmPublish": false, "pkgRoot": "SkyboxCreator_Project/Packages/SkyboxCreator" }],
    ["@semantic-release/git", {
      "assets": ["SkyboxCreator_Project/Packages/SkyboxCreator/package.json", "SkyboxCreator_Project/Packages/SkyboxCreator/CHANGELOG.md"],
      "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
    }],
    "@semantic-release/github"
  ]
}